{
  "name": "ywnsyage/clickhouse-php-client",
  "description": "Clickhouse client over HTTP and MySQL",
  "authors": [
    {
      "name": "ywnsyage",
      "email": "lost0719@163.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ywnsyage\\Clickhouse\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ywnsyage\\Clickhouse\\": "tests"
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "guzzlehttp/guzzle": "^6.0|^7.0"
  },
  "bin": [
    "bin/ccat_linux",
    "bin/ccat_darwin"
  ],
  "require-dev": {
    "mockery/mockery": "^0.9|^1.4",
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpunit/phpcov": "^5.0|^6.0|^7.0|^8.0"
  },
  "scripts": {
    "test": "phpunit --coverage-text --colors=never"
  },
  "config": {
    "sort-packages": true,
    "secure-http": false
  }
}
